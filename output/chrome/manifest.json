{
    "name": "OAS Circuit", 
    "icons": {
        "128": "icons/icon128.png", 
        "48": "icons/icon48.png", 
        "16": "icons/button.png"
    }, 
    "web_accessible_resources": [
        "res/*", 
        "assets/*", 
        "icons/*"
    ], 
    "description": "Centralized hub for receiving notifications from Orion Advisor Services and it's integration partners", 
    "background": {
        "page": "background.html"
    }, 
    "homepage_url": "http://orionadvisor.com/", 
    "version": "0.9.0", 
    "manifest_version": 2, 
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'", 
    "browser_action": {
        "default_popup": "popup.html", 
        "default_icon": "icons/button-gray.png", 
        "default_title": "Circuit"
    }, 
    "permissions": [
        "tabs", 
        "http://*/*", 
        "https://*/*", 
        "contextMenus", 
        "webNavigation", 
        "notifications"
    ]
}